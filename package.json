{
  "name": "tailwind-workstation-page",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "commit": "gacp",
    "dev": "vite",
    "format": "prettier --write \"**/*.{js,ts,css,less,json,md,html,yml,yaml,pcss,jsx,tsx}\"",
    "format:package": "sort-package-json",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "serve": "vite preview"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn lint"
    ],
    "package.json": [
      "yarn format:package"
    ]
  },
  "dependencies": {
    "alpinejs": "2.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.2.1",
    "autoprefixer": "10.2.4",
    "eslint": "7.19.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "gacp": "2.10.2",
    "husky": "5.0.8",
    "lint-staged": "10.5.3",
    "pinst": "2.1.4",
    "postcss": "8.2.4",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "sort-package-json": "1.48.1",
    "tailwindcss": "2.0.2",
    "vite": "2.0.0-beta.64"
  },
  "gacp": {
    "add": false,
    "push": false,
    "emoji": "emoji",
    "editor": false
  }
}
